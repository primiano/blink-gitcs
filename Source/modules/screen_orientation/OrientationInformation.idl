enum OrientationType {
    "portrait-primary",
    "portrait-secondary",
    "landscape-primary",
    "landscape-secondary"
};

[
    GarbageCollected,
    RuntimeEnabled=ScreenOrientation
] interface OrientationInformation {
    readonly attribute OrientationType type;
    readonly attribute unsigned short angle;
};
